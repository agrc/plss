{
  "name": "@ugrc/plss-app",
  "version": "3.6.0-2",
  "private": true,
  "description": "A React app for the UGRC PLSS",
  "license": "MIT",
  "author": "UGRC",
  "type": "module",
  "main": "./src/functions/index.mjs",
  "scripts": {
    "build": "vite build",
    "build-storybook": "storybook build",
    "cm": "cz",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "coverage": "vitest run --coverage",
    "dev": "cross-env NODE_ENV=development concurrently 'wait-on http://127.0.0.1:4000 && pnpm run dev:vite' 'pnpm run dev:firebase'",
    "dev:firebase": "cross-env NODE_ENV=development firebase emulators:start --import .emulator-data --only auth,functions,firestore,storage",
    "dev:vite": "vite",
    "fb:deploy": "firebase deploy --only functions",
    "fb:serve": "cross-env NODE_ENV=development firebase emulators:start --only functions",
    "format": "prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky",
    "preview": "vite preview",
    "save-firebase-state": "cross-env NODE_ENV=development firebase emulators:start --import .emulator-data --export-on-exit --only auth,functions,firestore,storage",
    "start": "pnpm run dev",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --silent --coverage --ui --open",
    "test:ci": "firebase emulators:exec --only storage 'FIREBASE_STORAGE_EMULATOR_HOST=127.0.0.1:9199 pnpm run test'"
  },
  "lint-staged": {
    "*.{js,jsx,mjs}": "eslint --cache --fix",
    "*.{md,yml,yaml,json}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.32.9",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^4.1.3",
    "@react-hook/window-size": "^3.1.1",
    "@tanstack/react-query": "^5.69.0",
    "@tanstack/react-query-devtools": "^5.69.0",
    "@ugrc/layer-selector": "^6.3.0",
    "@ugrc/utah-design-system": "^1.21.1",
    "@ugrc/utilities": "^3.0.2",
    "@xstate/react": "^5.0.3",
    "camelcase": "^8.0.0",
    "clsx": "^2.1.1",
    "contrast-color": "^1.0.1",
    "cors": "^2.8.5",
    "dms-conversion": "^3.1.3",
    "firebase": "^11.5.0",
    "if-env": "^1.0.4",
    "immer": "^10.1.1",
    "ky": "^1.7.5",
    "md5": "^2.3.0",
    "natural-compare-lite": "^1.4.0",
    "react": "^19.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "use-immer": "^0.11.0",
    "xstate": "^5.19.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@firebase/rules-unit-testing": "^4.0.1",
    "@hookform/devtools": "^4.3.3",
    "@storybook/addon-actions": "^8.6.9",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-interactions": "^8.6.9",
    "@storybook/addon-links": "^8.6.9",
    "@storybook/react": "^8.6.9",
    "@storybook/react-vite": "^8.6.9",
    "@tailwindcss/vite": "^4.0.15",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@ugrc/eslint-config": "^1.1.1",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/coverage-v8": "^3.0.9",
    "@vitest/ui": "^3.0.9",
    "commitizen": "^4.3.1",
    "concurrently": "^9.1.2",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.23.0",
    "firebase-functions-test": "^3.4.1",
    "firebase-tools": "^13.35.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prop-types": "^15.8.1",
    "storybook": "^8.6.9",
    "tailwindcss": "^4.0.15",
    "vite": "^6.2.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^3.0.9",
    "wait-on": "^8.0.3"
  },
  "engines": {
    "node": "22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@swc/core",
      "@vaadin/vaadin-usage-statistics",
      "core-js",
      "esbuild",
      "protobufjs",
      "re2"
    ]
  }
}
