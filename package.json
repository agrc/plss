{
  "name": "@ugrc/plss-app",
  "version": "3.6.12",
  "private": true,
  "description": "A React app for the UGRC PLSS",
  "license": "MIT",
  "author": "UGRC",
  "type": "module",
  "main": "./src/functions/index.mjs",
  "scripts": {
    "build": "vite build",
    "build-storybook": "storybook build",
    "cm": "cz",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "coverage": "vitest run --coverage",
    "dev": "cross-env NODE_ENV=development concurrently 'wait-on http://127.0.0.1:4000 && pnpm run dev:vite' 'pnpm run dev:firebase'",
    "dev:firebase": "cross-env NODE_ENV=development firebase emulators:start --import .emulator-data --only auth,functions,firestore,storage",
    "dev:firebase-state": "cross-env NODE_ENV=development concurrently 'firebase emulators:start --import=.emulator-data --export-on-exit=.emulator-data --only auth,functions,firestore,storage' 'pnpm run dev:vite'",
    "dev:vite": "vite",
    "fb:deploy": "firebase deploy --only functions",
    "fb:serve": "cross-env NODE_ENV=development firebase emulators:start --only functions",
    "format": "prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky",
    "preview": "vite preview",
    "save-firebase-state": "cross-env NODE_ENV=development firebase emulators:start --import .emulator-data --export-on-exit --only auth,functions,firestore,storage",
    "start": "pnpm run dev",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --silent --coverage --ui --open",
    "test:ci": "firebase emulators:exec --only storage 'pnpm test'"
  },
  "lint-staged": {
    "*.{js,jsx,mjs}": "eslint --cache --fix",
    "*.{md,yml,yaml,json}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.34.8",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@react-hook/window-size": "^3.1.1",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@ugrc/layer-selector": "^6.4.0",
    "@ugrc/utah-design-system": "^1.34.0",
    "@ugrc/utilities": "^3.1.1",
    "@xstate/react": "^6.0.0",
    "camelcase": "^9.0.0",
    "clsx": "^2.1.1",
    "contrast-color": "^1.0.1",
    "cors": "^2.8.6",
    "dms-conversion": "^3.1.4",
    "firebase": "^12.5.0",
    "if-env": "^1.0.4",
    "immer": "^11.0.1",
    "ky": "^1.14.3",
    "md5": "^2.3.0",
    "natural-compare-lite": "^1.4.0",
    "react": "^19.2.4",
    "react-color": "^2.19.3",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-hook-form": "^7.71.1",
    "use-immer": "^0.11.0",
    "xstate": "^5.25.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@hookform/devtools": "^4.4.0",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-links": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@ugrc/eslint-config": "^1.2.3",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.1",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.31.0",
    "firebase-functions-test": "^3.4.1",
    "firebase-tools": "^15.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "storybook": "^10.2.8",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^4.0.18",
    "wait-on": "^9.0.3"
  },
  "engines": {
    "node": "22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@swc/core",
      "core-js",
      "esbuild",
      "protobufjs",
      "re2",
      "unrs-resolver"
    ],
    "overrides": {
      "@eslint/plugin-kit@<0.3.4": ">=0.3.4",
      "@modelcontextprotocol/sdk@>=1.10.0 <=1.25.3": ">=1.26.0",
      "@modelcontextprotocol/sdk@<1.25.2": ">=1.25.2",
      "axios@<=1.13.4": ">=1.13.5",
      "body-parser@>=2.2.0 <2.2.1": ">=2.2.1",
      "diff@>=4.0.0 <4.0.4": ">=4.0.4",
      "form-data@>=4.0.0 <4.0.4": ">=4.0.4",
      "form-data@<2.5.4": ">=2.5.4",
      "hono@<4.11.4": ">=4.11.4",
      "hono@<4.11.7": ">=4.11.7",
      "jws@=4.0.0": ">=4.0.1",
      "lodash@>=4.0.0 <=4.17.22": ">=4.17.23",
      "lodash-es@>=4.0.0 <=4.17.22": ">=4.17.23",
      "on-headers@<1.1.0": ">=1.1.0",
      "qs@>=6.7.0 <=6.14.1": ">=6.14.2",
      "qs@<6.14.1": ">=6.14.1",
      "tar@<7.5.7": ">=7.5.7",
      "tar@<=7.5.2": ">=7.5.3",
      "tar@<=7.5.3": ">=7.5.4",
      "tmp@<=0.2.3": ">=0.2.4",
      "vite@>=7.1.0 <=7.1.10": ">=7.1.11"
    },
    "ignoredBuiltDependencies": [
      "@vaadin/vaadin-usage-statistics"
    ]
  }
}
